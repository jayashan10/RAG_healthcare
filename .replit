
modules = ["python-3.12"]

[interpreter]
[interpreter.command]
args = ["stdbuf", "-o", "L", "python3", "-m", "streamlit", "run", "app.py"]

[nix]
channel = "stable-24_05"

[deployment]
run = ["sh", "-c", "streamlit run app.py"]

